<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: #cccccc;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.StockManagerController">
    <children>
        <ListView fx:id="listView" layoutX="324.0" layoutY="9.0" onDragDetected="#displayPSU" prefHeight="684.0" prefWidth="926.0" />
        <TextField fx:id="nameField" layoutX="23.0" layoutY="43.0" promptText="Name" />
        <TextField fx:id="quantityField" layoutX="23.0" layoutY="76.0" promptText="Quantity" />
        <TextField fx:id="priceField" layoutX="23.0" layoutY="110.0" promptText="Price" />
        <ChoiceBox fx:id="categoryChoiceBox" layoutX="24.0" layoutY="292.0" onDragDetected="#displayPSU" />
        <Button layoutX="24.0" layoutY="360.0" mnemonicParsing="false" onAction="#addImage" onDragDetected="#displayPSU" prefHeight="25.0" prefWidth="98.0" text="Add Image" />
        <Button layoutX="24.0" layoutY="328.0" mnemonicParsing="false" onAction="#addStockItem" onDragDetected="#displayPSU" text="Add Stock Item" />
        <Button fx:id="removeButton" layoutX="24.0" layoutY="421.0" mnemonicParsing="false" onAction="#removeStockItem" onDragDetected="#displayPSU" prefHeight="25.0" prefWidth="98.0" text="Remove Item" />
      <Button layoutX="183.0" layoutY="453.0" mnemonicParsing="false" onAction="#displayMotherboard" prefHeight="25.0" prefWidth="132.0" text="Display Motherboard" />
        <Button layoutX="217.0" layoutY="292.0" mnemonicParsing="false" onAction="#displayCPU" prefHeight="25.0" prefWidth="98.0" text="Display CPU" />
        <Button layoutX="217.0" layoutY="325.0" mnemonicParsing="false" onAction="#displayRAM" prefHeight="25.0" prefWidth="98.0" text="Display RAM" />
        <Button layoutX="217.0" layoutY="359.0" mnemonicParsing="false" onAction="#displayGPU" prefHeight="25.0" prefWidth="98.0" text="Display GPU" />
      <Button layoutX="217.0" layoutY="391.0" mnemonicParsing="false" onAction="#displayPSU" prefHeight="25.0" prefWidth="98.0" text="Display PSU" />
      <Button layoutX="211.0" layoutY="421.0" mnemonicParsing="false" onAction="#displayStorage" prefHeight="25.0" prefWidth="104.0" text="Display Storage" />
      <Button fx:id="logoutButton" layoutX="267.0" layoutY="687.0" mnemonicParsing="false" onAction="#logoutManager" onDragDetected="#displayPSU" text="Logout" />
      <Button fx:id="editButton" layoutX="24.0" layoutY="391.0" mnemonicParsing="false" onAction="#editStockItem" onDragDetected="#displayPSU" prefHeight="25.0" prefWidth="98.0" text="Edit Item" />
      <Button layoutX="217.0" layoutY="259.0" mnemonicParsing="false" onAction="#displayAll" prefHeight="25.0" prefWidth="98.0" text="Display All" />
      <TextArea fx:id="descriptionField" layoutX="22.0" layoutY="145.0" prefHeight="108.0" prefWidth="297.0" promptText="Description" />
    </children>
</AnchorPane>
